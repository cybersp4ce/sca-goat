{
  "name": "lambda-api",
  "version": "0.1.2",
  "author": "Eran Medan",
  "repository": {
    "type": "git",
    "url": "git://github.com/aws-samples/amazon-cognito-example-for-external-idp.git"
  },
  "description": "The backend API for the demo app",
  "scripts": {
    "copy-resources": "./copy-resources.sh",
    "tsc-alias": "tsc-alias",
    "tsc": "tsc",
    "tsc:noemit": "tsc --noEmit",
    "nodemon": "nodemon",
    "ts-node": "ts-node",
    "prewatch": "npm run copy-resources",
    "watch": "nest build -w",
    "watch:tsc": "tsc -w",
    "watch/noemit": "tsc -w --noEmit",
    "start:nest": "NODE_NO_WARNINGS=1 nest start --watch --debug 4001",
    "start": "npm run watch & NODE_NO_WARNINGS=1 nodemon --inspect=4001 dist/lambda/api/src/local.js",
    "start:pause": "npm run watch & NODE_NO_WARNINGS=1 nodemon --inspect-brk=4001 dist/lambda/api/src/local.js",
    "prebuild": "npm run prisma:generate && npm run copy-resources",
    "build": "nest build",
    "build:tsc": "tsc -b && tsc-alias",
    "rebuild": "npm run clean && npm ci && npm run build",
    "clean": "tsc -b --clean && rm -rf dist",
    "package": "cd dist/src && zip -r ../function.zip .",
    "test/test": "RUN_LOCALLY=true mocha",
    "test/spec": "RUN_LOCALLY=true mocha",
    "test": "RUN_LOCALLY=true mocha",
    "testWithCoverage": "nyc -e .ts -x \"*.test.ts\" -x \"*.spec.ts\" mocha && nyc report",
    "coverage": "nyc npm test",
    "coverage:shared": "nyc --temp-dir='../../.nyc_output' --silent --no-clean npm test",
    "lint": "eslint ./{src,tests}/**/* --ext .ts",
    "stripe-local": "stripe listen --skip-verify --forward-to https://localhost:3001/callbacks/payment/stripe",
    "db-admin": "dynamodb-admin",
    "set-tier": "ts-node ./tests/utils/set-tier.ts",
    "get-token": "ts-node ./tests/utils/get-token.ts",
    "prisma:generate": "prisma generate #generates the prisma client",
    "prisma:migrate-create": "prisma migrate dev --name \"$(date +%s)\" #creates a new migration file and runs it, also runs generate",
    "prisma:migrate-apply": "prisma migrate dev #applies the migrations locally",
    "prisma:migrate-deploy": "prisma migrate deploy #applies the migration to the database, also runs generate",
    "prisma:studio": "prisma studio #opens the prisma studio",
    "prisma:push": "prisma db push #pushes the schema to the database, also runs generate",
    "prisma:reset": "prisma db push --force-reset #resets the database, also runs generate",
    "prisma:format": "prisma format #formats the schema file"
  },
  "prisma": {
    "schema": "src/sql/prisma/schema.prisma"
  },
  "keywords": [],
